{
  "name": "front-end-boilerplate",
  "description": "A modern front-end boilerplate utilising: Parcel, Tailwind and Vue",
  "keywords": [],
  "author": "Aaron George <aaron.dean.george@gmail.com> (https://github.com/aarongeorge)",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build:broken": "npx parcel build ./src/index.html --no-source-maps",
    "build": "npx parcel build ./src/index.html --no-source-maps --no-minify",
    "clean": "rm -rf ./dist ./.cache",
    "serve:parcel": "npx parcel serve ./src/index.html",
    "serve": "cd ./dist && npx serve ./"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "parcel-bundler": "^1.12.3",
    "postcss-import": "^12.0.1",
    "serve": "^11.1.0",
    "tailwindcss": "^1.1.2",
    "tailwindcss-transitions": "^2.1.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "nprogress": "^0.2.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1"
  },
  "browser": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  }
}
